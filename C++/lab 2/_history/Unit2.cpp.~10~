void __fastcall TForm2::OpenClick(TObject *Sender)
{
	if (!OpenDialog1->Execute()){
		ShowMessage("You didn't choose anything.");
		return;
	}
	List->LoadFromFile(OpenDialog1->FileName);
    Table->LoadFromSile(OpemDialog1->FileName);
}
